{
  "name": "betterGS",
  "version": "0.1.1",
  "author": "Stadt Goslar - MachMit!Haus",
  "homepage": "https://machmit.goslar.de",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:de": "ng serve --configuration=development --aot --i18n-locale=de --i18n-file=src/locale/messages.de.xlf --i18n-format=xlf",
    "start:en": "ng serve --configuration=development --aot --i18n-locale=en --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf",
    "build": "ng build",
    "build:i18n": "npm run build:en && npm run build:de",
    "build:de": "ng build --configuration=production --aot --i18n-locale=de --i18n-file=src/locale/messages.de.xlf --i18n-format=xlf --output-path=www/de",
    "build:en": "ng build --configuration=production --aot --i18n-locale=en --i18n-file=src/locale/messages.en.xlf --i18n-format=xlf --output-path=www/en",
    "extract-i18n": "ng extract-i18n --output-path=src/locale",
    "watch": "ng build --watch --configuration development,de",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.0.0",
    "@angular/localize": "^20.0.6",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/core": "7.4.1",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "7.4.1",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/local-notifications": "^7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@ionic/angular": "^8.0.0",
    "@ionic/pwa-elements": "^3.3.0",
    "ionicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "unique-names-generator": "^4.7.1",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular-eslint/builder": "^20.0.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular-eslint/eslint-plugin-template": "^20.0.0",
    "@angular-eslint/schematics": "^20.0.0",
    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/language-service": "^20.0.0",
    "@capacitor/cli": "7.4.1",
    "@ionic/angular-toolkit": "^12.0.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.0"
  },
  "description": "An Ionic project"
}
